{
  "expo": {
    "name": "CGM",
    "slug": "cgm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "react-native-nfc-manager",
        {
          "isProvideEnable": true
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.cgm",
      "infoPlist": {
        "NFCReaderUsageDescription": "This app uses NFC to scan glucose monitoring sensors",
        "com.apple.developer.nfc.readersession.iso15693.app": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.cgm",
      "permissions": [
        "android.permission.NFC"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c37a98b4-502a-47b2-974c-7829dd8dfb21"
      }
    }
  }
}
